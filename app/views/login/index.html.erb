
<div class="login_form">
  <h1 class="">Login</h1>
  <%= form_tag(authentication_login_index_path, method: :get) do |f| %>
    <div class="field">
      <%= label_tag :email %><br>
      <%= text_field_tag :email_id %>
    </div>
    <div class="actions">
      <%= submit_tag "Login"%>
    </div>
  <% end %>
</div>